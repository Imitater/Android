<?xml version="1.0" encoding="utf-8"?>
<!--会员 fragemnt-->

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="2"
    android:orientation="horizontal">
    <LinearLayout
        android:orientation="vertical"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="match_parent">
        <Button
            android:id="@+id/car_add"
            android:text="添加网络购物车"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/car_del"
            android:text="删除网络购物车"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/car_query"
            android:text="查询网络购物车"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/car_update"
            android:text="修改网络购物车"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/car_Activity"
            android:text="购物车Activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Button
            android:text="评论"
            android:id="@+id/bt_pinglun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="二维码"
            android:id="@+id/bt_erweima"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:text="自定义吐司"
            android:id="@+id/bt_toast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:text="代金券"
            android:id="@+id/bt_djq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
        android:id="@+id/tv_erweima"
        android:text="二维码显示："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
        <Button
            android:text="测试订单"
            android:id="@+id/bt_dingdan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <Button
            android:text="微信分享"
            android:id="@+id/bt_wxfx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tv_data"
            android:text="data：显示"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="match_parent">

    <Button
        android:id="@+id/wx_sq"
        android:text="获取微信授权"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/qq_sq"
        android:text="获取QQ授权"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/loin_activiy"
        android:text="登陆界面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/add_shoppingcar"
        android:text="添加购物车"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/add_personinfo"
        android:text="个人信息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/add_vitamio"
        android:text="网络视频"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/Map_get"
        android:text="地图"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

        <Button
            android:id="@+id/Contact_get"
            android:text="联系人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    <Button
        android:id="@+id/video"
        android:text="播放视频页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/audio"
        android:text="播放图书页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/vip"
        android:text="Vip页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/toy_suit"
        android:text="套餐详情页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/toy_detail"
        android:text="玩具详情页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/test_network"
        android:text="测试网络"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    </LinearLayout>

</LinearLayout>

    </ScrollView>

</LinearLayout>