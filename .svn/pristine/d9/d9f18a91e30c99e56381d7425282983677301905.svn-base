<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/transparent</item>
        <item name="colorPrimaryDark">@color/tab_selected</item>
        <item name="colorAccent">@color/blue_dark</item>

        <item name="android:statusBarColor" tools:targetApi="lollipop">@color/black</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>

    </style>

    <style name="BottomDialog" parent="@style/AppTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="MaterialDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:tint">@color/blue_dark</item>
    </style>

    <style name="AlertWidth">
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginRight">50dp</item>
        <item name="android:textSize">@dimen/small_size</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <!--注册页面的EditText-->
    <style name="EditText_style">
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/item_size</item>
        <item name="android:ems">10</item>
        <item name="android:background">@null</item>
    </style>
    <style name="dialog_style">
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 是否显示title -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景是否模糊显示 -->
        <item name="android:backgroundDimEnabled">false</item>

    </style>

    <style parent="@android:style/Theme.Dialog" name="UIAlertViewStyle">
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowIsFloating">true</item>
    <item name="android:windowFrame">@null</item>
    <item name="android:backgroundDimEnabled">true</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">true</item>



</style>

    <style name="MainRadioButtonStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:textSize">12sp</item>

    </style>

    <style name="mime_item_iv">
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_width">22dp</item>
    </style>

    <style name="mine_item_tv">
        <item name="android:textSize">@dimen/item_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">11dp</item>
        <item name="android:layout_marginStart">11dp</item>
    </style>

    <!--标题relativeLayout-->
    <style name="title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_hetght</item>
        <item name="android:background">@color/Panda_BG</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
    <!--back返回imageButton-->
    <style name="back_style">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:padding">25dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:id">@id/back</item>
        <item name="android:background">@mipmap/back</item>
    </style>
    <!--标题TextView-->
    <style name="title_textview_style">
        <item name="android:textColor">#2c2c2c</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/title_size</item>
    </style>

    <!--活动页面样式-->
    <style name="event_ll">
        <item name="android:padding">3dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="event_ll_title">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="event_textview_left">
        <item name="android:textSize">@dimen/sub_title_size</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="event_textview_right">
        <item name="android:textSize">@dimen/sub_title_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="event_recycler">
        <item name="android:layout_marginLeft">7dp</item>
        <item name="android:layout_marginRight">7dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="event_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="event_rl">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginBottom">7dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--课程页面条目布局-->
    <style name="class_cardview">
        <item name="android:background">@color/white</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="class_LinearLayout">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="class_imageview">
        <item name="android:src">@mipmap/item_background</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="class_iv_play">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="class_textview">
        <item name="android:lines">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/item_size</item>
    </style>

    <!--VIP页面条目布局-->
    <style name="vip_ll">
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="vip_imageview">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">100dp</item>
    </style>
    <style name="vip_ll_info">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="vip_info_tv1">
        <item name="android:textColor">@color/tab_selected</item>
        <item name="android:textSize">@dimen/title_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="vip_info_tv2">
        <item name="android:textColor">@color/grad</item>
        <item name="android:textSize">@dimen/small_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>
    <style name="vip_register">
        <item name="android:padding">7dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/item_size</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginTop">60dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:text">会员权益</item>
    </style>


    <!--dialog满屏
        <item name="android:windowFullscreen">true</item>-->
    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Dialog_shared">
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="Dialog_Full">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="popupDialog">
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowFullscreen">true</item>
</style>
    <style name="Dialog_Find_Image">
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/dialogWindowAnim</item>

    </style>

   <!-- //dialog的动画-->
    <style name="dialogWindowAnim" parent="android:Animation" mce_bogus="1">
        <item name="android:windowEnterAnimation">@anim/find_dialog_show_anim</item>
        <item name="android:windowExitAnimation">@anim/find_dialog_dismiss_anim</item>
    </style>

</resources>
